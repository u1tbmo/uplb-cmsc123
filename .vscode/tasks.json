{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "Exercise 6 Compilation",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "-c",
                "template.c",
                "BST.c"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        },
        {
            "label": "Exercise 6 Object Linking",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-o",
                "main",
                "template.o",
                "BST.o"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "dependsOn": "Exercise 6 Compilation",
            "problemMatcher": ["$gcc"]
        }
    ],
    "version": "2.0.0"
}